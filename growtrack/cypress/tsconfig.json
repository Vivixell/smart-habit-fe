{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "dom"],
      "module": "commonjs",
      "moduleResolution": "node",
      "types": ["cypress"],
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "allowJs": true
    },
    "include": ["**/*.ts", "**/*.tsx"]
  }